.container{
  display: flex;
  width: 100%;
  height: 1230rpx;
  overflow: hidden;
}
/*开始界面显示样式*/
.start{
  width: 100%;
  height: 100%;
  text-align: center;
  background-color: #000;
  background-image: url("../assets/image/start_background.png");
  background-size: cover;
  overflow: hidden;
}

/*开始界面Button样式*/
.start button{
  position: absolute;
  top: 1000rpx;
  left: 240rpx;
  width: 300rpx;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  border: solid 1rpx black;
  border-radius: 15rpx;
  background-color: #c4c9ca;
}

/*主界面背景*/
.main{
  background-image: url("../assets/image/background_1.png");
  width: 100%;
  height: 100%;
  background-size: cover;
  overflow: hidden;
 
}

/*分数显示父布局*/
.score{
  font-size: 16px;
  z-index: 999;
  font-weight: bold;
  position: absolute;
  top: 10px;
  left: 10px;
}

.end{
  position: absolute;
  top: 210px;
  left: 75px;
  border: 1rpx solid gray;
  border-radius: 5px;
  text-align: center;
  background-color: #d7ddde;
  display: none;
  z-index: 999;
}

.end view{
  width: 160px;
  height: 50px;
}

.end view button{
  margin-top: 10px;
  position: absolute;
  left: 20px;
  width: 300rpx;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  border: 1rpx solid black;
  border-radius: 30px;
  background-color: #c4c9ca;
}

.plantext{
  width: 160px;
  height: 50px;
  line-height: 50px;
  font-size: 16px;
  font-weight: bold;
  text-align: center;
}

.planscores{
  width: 160px;
  height: 50px;
  text-align: center;
  line-height: 50px;
  border-top: 1rpx solid gray;
  border-bottom: 1rpx solid gray;
  font-size: 16px;
  font-weight: bold;
}